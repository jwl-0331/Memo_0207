<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
 PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
 "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
 
<mapper namespace="com.jwl.memo.post.PostDAO">
	<insert id="insertPost" parameterType="map">
		INSERT INTO
			`post`
		(
			`userId`
			, `title`
			, `content`
			, `createdAt`
			, `updatedAt`
		)
		VALUE
		(
			#{userId}
			, #{title}
			, #{content}
			, now()
			, now()
		)
	</insert>
</mapper>